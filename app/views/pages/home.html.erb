<% if signed_in? %>

	<div class="left-container">

		<% unless current_user.feed.empty? %>

			<div class="video-feed-header">Videos from your followers</div>
				<div id="results">
					<%= render 'shared/feed' %>
					<%= will_paginate @feed_items %>
					<%= pageless(@feed_items.total_pages, root_path, "#results") %>
				</div>
		<% else %>

			<div>NOT FOLLOWING ANYONE</div>

		<% end %>

	</div>





<% else %>

<div class="left-container">
	<div class="video-feed-header">Top Videos</div>
	<div id="results">
		<%= render 'shared/feed' %>
		<%= will_paginate @feed_items %>
		<%= pageless(@feed_items.total_pages, videos_path, "#results") %>
	</div>
</div>

<% end %>